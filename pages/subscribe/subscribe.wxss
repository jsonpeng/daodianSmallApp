/* pages/subscribe/subscribe.wxss */
.navbar{
  display:flex;
  background-color: #fff;
}
.navbar .item{
  flex:1;
  text-align: center;
  padding:20rpx 0;
  font-size: 15px;
}
.navbar .item text{padding:20rpx 0;}
.navbar .active text{
  color:#e6be78;
  border-bottom: 1px solid #e6be78;
}
.my_subscribe{padding:7.5px 7.5px 0 7.5px;}
.subscribe-item{border-radius:5px;background-color:#fff;overflow: hidden;margin-bottom: 30rpx;}
.subscribe-item .sub-time{height:90rpx;line-height: 90rpx;font-size:15px;color: #e6be78;padding:0 7.5px;background-image: url(https://ajls3lyu.shop.qijianshen.xyz/uploads/sub_bgi.jpg);background-repeat: no-repeat;background-position:center;background-size:cover;}
.subscribe-item .technician{font-size: 14px;padding:5px 7.5px;}
.technican-info image{width: 35px;}
.technican-info .weui-cell__hd{border-radius: 50%;overflow:hidden;margin:0 8px;}
.technican-info .technician-name{font-size: 18px;}
.technican-info .technician-intr{font-size:12px;color:#9c9c9c;}
.technican-info .weui-cell__bd{padding-left:10px;}
.store-info{text-align: left;}
.store-info .weui-cell__hd{display: flex;align-items: center;}
.store-info .store-pic{width:36px;}
.store-info .location{width: 12px;margin:0 5px;}
.store-info .store-add{padding:0;font-size:12px;color: #999;}
.store-info .store-name{font-size: 16px;}
.subscribe-item{position: relative;}
.subscribe-item .btn-list{display: flex;justify-content: space-between;}
.subscribe-item .btn-list view{border: 1px solid #e6be78;border-radius:15px;padding:2px 10px;color: #e6be78;display: flex;align-items: center;font-size: 14px;}
.subscribe-item .btn-list image{width:15px;margin-right: 3px;}
.checkwrapper .right-botton01{background-color:#e6be78;}
.status_img{width: 170rpx;height:126rpx;position: absolute;right: 0;top: 170rpx;overflow: hidden;}
.status_img image{width: 100%;}

/* 技师列表 */
.box{padding: 0 15rpx;margin-top:15rpx;}
.technician_list{padding: 15rpx 0;background-color: #fff;margin-bottom: 10rpx;}
.technician_list .weui-cell__hd{padding:0 25rpx;}
.technician_list .weui-cell__bd{padding:0 25rpx;border-left: 1px solid #ececec;}
.technician_list .locaiton{width:25rpx;}
.technician_list .via{width: 135rpx;height: 135rpx;border-radius: 50%;overflow: hidden;margin: 0 auto;margin-bottom: 10rpx;}
.technician_list .via image{width:100%;height: 100%;}
.technician_list .add{font-size: 12px;color:#9c9c9c;text-align: center;display: flex;align-items: center;max-width:10em;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;}
.technician_list .name{font-size:20px;font-weight:bold;color: #333;margin-bottom: 15rpx;}
.technician_list .name text{font-size: 14px;color:#333;margin-left: 30rpx;font-weight: normal;}
.technician_list .intr{font-size: 14px;color: #9c9c9c;margin-bottom: 20rpx;}
.technician_list .phone{width:30rpx;}
.technician_list .praise{margin: 0;}
.technician_list .praise image{width: 30rpx;}
.technician_list .contact{display: flex;align-items: center;justify-content:space-between;}
.technician_list .contact .tel,.technician_list .contact .praise{display: flex;align-items: center;}